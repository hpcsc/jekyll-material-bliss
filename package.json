    "babel-plugin-closure-elimination": "^1.1.14",
    "babel-plugin-constant-folding": "^1.0.1",
    "babel-plugin-material-ui": "^0.3.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.16.0",
    "classnames": "^2.2.5",
    "dedupe-plugin": "^1.0.0",
    "dompurify": "^0.8.4",
    "ejs": "^2.5.5",
